[access]
	inheritFrom = All-Projects
[submit]
[access "refs/*"]
	owner = group user/HeroBuxx (herobuxx)
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/HeroBuxx (herobuxx)
	label-Verified = -1..+1 group user/HeroBuxx (herobuxx)
	push = group user/HeroBuxx (herobuxx)
	read = group user/HeroBuxx (herobuxx)
	rebase = group user/HeroBuxx (herobuxx)
	removeReviewer = group user/HeroBuxx (herobuxx)
	submit = group user/HeroBuxx (herobuxx)
[access "refs/heads/*"]
	create = group user/HeroBuxx (herobuxx)
	push = +force group user/HeroBuxx (herobuxx)
	pushMerge = group user/HeroBuxx (herobuxx)
	read = group user/HeroBuxx (herobuxx)
[access "refs/tags/*"]
	push = group user/HeroBuxx (herobuxx)
	push = +force group user/HeroBuxx (herobuxx)
